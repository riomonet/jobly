{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "company_handle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "equity": {
      "type": "numeric"
    },
    "salary": {
      "type": "integer",
	"minimum": 0
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "company_handle",
      "salary",
      "equity"
  ]
}
